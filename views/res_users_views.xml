<odoo>
    <record id="view_users_form_inherit_team" model="ir.ui.view">
        <field name="name">res.users.form.inherit.team</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">

            
            <xpath expr="//page[@name='access_rights']" position="inside">
                <group string="Football">
                    <field name="team_id"/>
                </group>
            </xpath>

        </field>
    </record>
</odoo>