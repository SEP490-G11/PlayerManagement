<data>
    <record id="z_payos_logs" model="ir.ui.view">
        <field name="name">z_payos.qr_logs</field>
        <field name="model">z_payos.qr.logs</field>
        <field name="arch" type="xml">  
           <tree string="QR Logs" js_class="tree_with_toast" create="0" edit="0" delete="0" default_order="create_date desc">
                    <field name="payment_channel"/>
                    <field name="payment_amount"/>
                    <field name="move_id" widget="many2one_tags" readonly="1"  />
                    <field name="place" />
                    <field name="invoice_amount"/>
                    <field name="create_date"/>
                    <field name="payment_description"/>
                    <field name="account_bank"/>
                    <field name="payment_code"/>
                    <field name="payment_status"/>     
                    <field name="is_create_account_payment" /> 
                    <button ></button>
            </tree>
        </field>
    </record>
    <!-- # tạo action để mở list view -->
    <record id="z_payos_logs_action" model="ir.actions.act_window">
        <field name="name">QR List</field>
        <field name="res_model">z_payos.qr.logs</field>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="z_payos_logs"/>
    </record>

    <record id="z_action_journal_id" model="ir.actions.act_window">
        <field name="name">Journal</field>
        <field name="res_model">account.journal</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!-- #tạo menu item cho list view trên -->
    <menuitem id="menu_z_payos_logs" name="QR Logs" parent="account.menu_finance" action="z_payos_logs_action" sequence="1000"/>
    <menuitem id="menu_z_journal" name="Journal"  parent="account.account_banks_menu" groups="account.group_account_manager" action="z_action_journal_id" sequence="1000"/>
</data>
