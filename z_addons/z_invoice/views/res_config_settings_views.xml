<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_config_settings_view_form_inherit_misa" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.misa</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="base.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//block[@name='performance']" position="after">
                    <block title="MISA config" name="system_misa_config">
                         <setting id="misa_api_url">
                            <field name="misa_api_url"/>
                        </setting>
                         <setting id="misa_api_timeout">
                            <field name="misa_api_timeout"/>
                        </setting>
                    </block>
                </xpath>
            </field>
        </record>
    </data>
</odoo> 