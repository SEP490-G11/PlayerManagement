<odoo>
    <data>
        <record id="preview_iframe_view" model="ir.ui.view">
            <field name="name">z_web.iframe</field>
            <field name="model">z_web.iframe</field>
            <field name="arch" type="xml">
                <form string="upload file wizard" class="image-result-wizard" > 
                        <field name="url" widget="button_upload_result" />
                        <footer>
                            <button string="Đóng" class="btn btn-secondary"
                                special="cancel" help="Cancel Upload" />
                        </footer>
                </form>
            </field>
        </record>


        <record id="preview_iframe_view_action" model="ir.actions.act_window">
            <field name="name">Kết quả</field>
            <field name="view_mode">form</field>
            <field name="res_model">z_web.iframe</field>
            <field name="target">new</field>
        </record>
    </data>
</odoo>